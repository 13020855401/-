.container{
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.top{
  width: 100%;
  height: 827rpx;
  position: absolute;
  top: 0;
}
.middle{
  position: absolute;
  top: 0rpx;
  width: 100%;
}
.bottom{
  width: 100%;
  height: 216rpx;
  position: absolute;
  bottom: 0;
}
.logo{
  width: 180rpx;
  height: 60rpx;
  position: absolute;
  bottom: 0px;
  left: 50%;
  margin-left: -90rpx;
}
.contact{
  position: absolute;
  bottom: 50rpx;
}
.btns{
  position: absolute;
  top: 420rpx;
  width: 100%;
  text-align: center;
}
.selectBtn{
  width: 100%;
  position: relative;
  text-align: center;
  margin-bottom: 35rpx;
}
.selectBtn image{
  width: 433rpx;
  height: 83rpx;
}
.selectBtn view{
  position: absolute;
  top: 10rpx;
  text-align: center;
  width: 100%;
  display:block;
left:0;
}
.selectBtn input{
  width:50px;
  display:inline-block;
  vertical-align:middle;
  border-bottom:1px solid #000;
  font-size: 44rpx;
}
.tip{
  position: absolute;
  top: 365rpx;
  text-align: center;
  width: 100%;
  display: block;
}
.next{
  position: absolute;
  bottom: 265rpx;
  margin-left: -42rpx;
  padding: 0;
  margin:0;
  background: none;
  border: 0; 
  border-radius: 121rpx;
}
.next,.next image{
  width: 243rpx;
  height: 84rpx;
}